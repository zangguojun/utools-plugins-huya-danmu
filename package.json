{
  "name": "reactjs-vite-tailwindcss-boilerplate",
  "description": "This is a boilerplate build with React 18, TypeScript, Jest, Testing Library, TailwindCSS 3, Eslint and Prettier.",
  "version": "0.1.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
